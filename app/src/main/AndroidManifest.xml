<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kakacat.minitool">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:requestLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="q">
        <activity android:name=".expressinquiry.activity.DeliveryDetailActivity" />
        <activity
            android:name=".expressinquiry.activity.ExpressInquiryActivity"
            android:theme="@style/MyMaterialNoActionBarStyle" />

        <service
            android:name=".audiocapture.GetAudioService"
            android:exported="false" />

        <activity android:name=".translation.TranslationActivity" />
        <activity
            android:name=".epidemicinquiry.EpidemicInquiryActivity"
            android:theme="@style/MyMaterialNoActionBarStyle" />
        <activity
            android:name=".garbageclassify.GarbageClassificationActivity"
            android:theme="@style/MyMaterialNoActionBarStyle" />
        <activity
            android:name=".cleanfile.CleanFileActivity"
            android:theme="@style/MyMaterialNoActionBarStyle" />
        <activity
            android:name=".textencryption.TextEncryptionActivity"
            android:theme="@style/MyMaterialNoActionBarStyle" />
        <activity android:name=".appInfo.activity.AppInfoActivity" />
        <activity android:name=".wifipasswordview.WifiPwdActivity" />
        <activity
            android:name=".todayinhistory.TodayInHistoryActivity"
            android:label="@string/title_today_in_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".phoneartribution.PhoneAttributionActivity"
            android:theme="@style/MyMaterialNoActionBarStyle" />
        <activity
            android:name=".inquireip.InquireIpActivity"
            android:theme="@style/MyMaterialNoActionBarStyle" />
        <activity
            android:name=".currencyconversion.MainActivity"
            android:theme="@style/MyMaterialNoActionBarStyle"
            />
        <activity
            android:name=".main.MainActivity"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>